[
  {
    "grid": "G1",
    "title": "获取地震工区筛选条件",
    "query": {
      "query": {
        "name": "地震工区"
      },
      "fields": {
        "properties.海域状况": 1,
        "properties.作业方式2": 1,
        "properties.作业类型": 1,
        "properties.采集时间": 1
      }
    }
  },
  {
    "grid": "G3",
    "title": "获取筛选后的地震工区成果列表",
    "query": {
      "_source":["swa","pt","iiid","title","indexeddate","thumbnail"],
      "query": {
        "match": { "pt.keyword": "地震工区位置图" }
      },
      "post_filter": {
        "terms": {
          "swa": "@swa"
        }
      },
      "from": "@from",
      "size": "@size",
      "sort": [
        {
          "indexeddate": {
            "order": "desc"
          }
        }
      ]
    }
  }
]
